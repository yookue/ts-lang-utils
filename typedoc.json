{
    "name": "TsLangUtils",
    "entryPoints": [
        "src/index.ts"
    ],
    "disableSources": true,
    "excludeExternals": true,
    "excludeInternal": true,
    "excludePrivate": true,
    "excludeProtected": true,
    "excludeNotDocumented": true,
    "excludeNotDocumentedKinds": [
        "ConstructorSignature"
    ],
    "hideGenerator": true,
    "includeVersion": false,
    "navigationLinks": {
        "GitHub": "https://github.com/yookue/ts-lang-utils",
        "Yookue": "https://yookue.com"
    },
    "plugin": [
        "@yookue/typedoc-plugin-raw-content"
    ],
    "rawHeadEnds": [
        "<meta name='keywords' content='typescript, yookue, ts-lang-utils'/>"
    ],
    "rawFooterEnds": [
        "<div style='text-align: center'>Copyright &copy; 2023 Yookue Ltd. All rights reserved</div>"
    ]
}
