{
    "source": {
        "include": [
            "docs-temp",
            "README.md"
        ],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc", "closure"]
    },
    "opts": {
        "template": "node_modules/docdash",
        "encoding": "utf-8",
        "destination": "./docs-dist",
        "recurse": true,
        "verbose": false
    },
    "plugins": [
        "plugins/markdown"
    ],
    "docdash": {
        "openGraph": {
            "title": "TsLangUtils",
            "type": "website",
            "image": "",
            "site_name": "ts-lang-utils",
            "url": "https://yookue.github.io/ts-lang-utils"
        },
        "meta": {
            "title": "TsLangUtils",
            "description": "Common Lang Utilities for TypeScript",
            "keyword": "yookue, commons-lang, array-utils, boolean-utils, object-utils, random-utils, string-utils, time-utils"
        },
        "menu": {
            "Github": {
                "href": "https://github.com/yookue/ts-lang-utils",
                "target": "_blank",
                "class": "menu-item",
                "id": "repo_link"
            }
        },
        "search": true,
        "templates": {
            "default": {
                "includeDate": false,
                "outputSourceFiles": false
            },
            "cleverLinks": false,
            "monospaceLinks": false
        }
    }
}
